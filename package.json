{
  "name": "iptv-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "novu:dev": "novu dev --port 3000",
    "email:dev": "email dev --port 3001",
    "db:generate": "drizzle-kit generate --dialect=postgresql --schema=database/schema.ts --out=./database/drizzle",
    "db:migrate": "tsx ./database/migrate.ts",
    "db:studio": "drizzle-kit studio --config=drizzle.config.ts",
    "db:push": "drizzle-kit push --config=drizzle.config.ts",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:build": "docker build -f Dockerfile -t iptv-dashboard --target dashboard $(for i in `cat .env`; do out+=\"--build-arg $i \" ; done; echo $out;out=\"\") ."
  },
  "dependencies": {
    "@ag-grid-community/client-side-row-model": "^32.0.2",
    "@ag-grid-community/core": "^32.0.2",
    "@ag-grid-community/react": "^32.0.2",
    "@ag-grid-community/styles": "^32.0.2",
    "@ag-grid-enterprise/master-detail": "^32.0.2",
    "@auth/drizzle-adapter": "^1.4.1",
    "@mdxeditor/editor": "^3.11.1",
    "@nextui-org/react": "^2.4.6",
    "@novu/framework": "2.0.0-canary.6",
    "@novu/node": "2.0.0-canary.1",
    "@novu/react": "2.0.0-canary.1",
    "@react-email/components": "^0.0.21",
    "@uiw/react-color-compact": "^2.3.0",
    "ag-grid-react": "^32.0.2",
    "apexcharts": "^3.50.0",
    "bcrypt": "^5.1.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "drizzle-orm": "^0.32.0",
    "easy-rbac": "^3.2.0",
    "formik": "^2.4.6",
    "framer-motion": "^11.3.8",
    "hono": "^4.5.1",
    "iconsax-react": "^0.0.8",
    "lodash": "^4.17.21",
    "next": "14.2.5",
    "next-auth": "5.0.0-beta.19",
    "next-themes": "^0.3.0",
    "nodemailer": "^6.9.14",
    "nuqs": "^1.17.6",
    "papaparse": "^5.4.1",
    "postgres": "^3.4.4",
    "react": "^18",
    "react-apexcharts": "^1.4.1",
    "react-dom": "^18",
    "react-email": "^2.1.5",
    "short-unique-id": "^5.2.0",
    "sonner": "^1.5.0",
    "yup": "^1.4.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.2"
  },
  "devDependencies": {
    "@next/env": "^14.2.5",
    "@types/bcrypt": "^5.0.2",
    "@types/easy-rbac": "^3.1.5",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.15",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.23.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "novu": "2.0.0-canary.9",
    "postcss": "^8",
    "serverless": "3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
